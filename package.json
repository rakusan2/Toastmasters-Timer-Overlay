    "open": "~7.0.3"
  },
  "devDependencies": {
    "@types/node": "~13.11.1",
    "@types/node-static": "~0.7.3",
    "@types/socket.io": "~2.1.4",
    "@types/socket.io-client": "~1.4.32",
    "pkg": "^4.4.8",
    "ts-loader": "~7.0.4",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "~3.8.3",
    "webpack": "~4.43.0",
    "webpack-cli": "~3.3.11",
    "webpack-merge": "~4.2.2"
